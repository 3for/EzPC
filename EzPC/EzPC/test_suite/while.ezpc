(*
	This example shows the usage of while loop.
*)

def void main(){
	int32_al[10] yy;

	(*
		Following while loop will execute until the condition (i<10) is true.
		In general, the condition can be any arbitrary expression evaluating to public boolean.
	*)

	yy[0] = 0;
	int32_pl i = 1;
	while (i < 10)
	{
		yy[i] = yy[i-1] + 1;
		i = i + 1;
	};
	output(ALL, yy[9]);
}